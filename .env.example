# KWANUS OS: Environmental Signals
# Copy this file to .env and populate with Nexus secrets.

# 1. CORE DATABASE (PostgreSQL)
DATABASE_URL="postgresql://user:password@localhost:5432/kwanus_nexus"

# 2. IDENTITY & AUTH
NEXTAUTH_SECRET="RESONANCE_SECRET_NEEDS_GENERATION"
NEXTAUTH_URL="http://localhost:3000"

# 3. NEXUS TELEMETRY
ANALYTICS_KEY="NEXUS_ANALYTICS_PLACEHOLDER"

# 4. EMOTIONAL INTENSITY OVERRIDES (Dev Mode)
DEFAULT_EMOTIONAL_INTENSITY=0.5
DEBUG_MODE=true
